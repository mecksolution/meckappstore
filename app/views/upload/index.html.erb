<h1>Listing File Uploads</h1>

<table>
  <tr>
    <th>File</th>
   
    <th></th>
  </tr>

<% @uploads.each do |upload| %>
  <tr>
    <td><%= upload.attach.url %></td>
   
    <td><%= link_to 'Download', upload %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Upload', new_upload_path %>
